    "def fibonocci(n):\n",
    "    a=0\n",
    "    b=1\n",
    "    if n < 0:\n",
    "        return \"Incorrect number\"\n",
    "    elif n==1:\n",
    "        return a\n",
    "    elif n==2:\n",
    "        return b\n",
    "    else:\n",
    "        return fibonocci(n-1)+fibonocci(n-2)"
   ]
